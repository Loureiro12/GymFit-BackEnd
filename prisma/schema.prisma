// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model StudentUser {
  id        String  @id @default(uuid())
  firstName String
  lastName  String
  email     String  @unique
  password  String
  gender    String?
  birthday  String?
  weight    String?
  height    String?
  goal      String?

  @@map("studentUsers")
}
